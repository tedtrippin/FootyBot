<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD//EN"
		"http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">

<!--
  - Mapping file for the Hibernate implementation of the Clinic interface.
	-->
<hibernate-mapping auto-import="true" default-lazy="false">

	<class name="com.rob.betBot.persistence.RaceTime" table="racetime">
		<id name="id" column="id">
			<generator class="identity"/>
		</id>
		<property name="raceName" column="race_name"/>
		<property name="durationMs" column="last_name"/>
	</class>

</hibernate-mapping>
